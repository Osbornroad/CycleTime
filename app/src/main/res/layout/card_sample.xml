<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/sample_employee_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Employee name"/>
        <TextView
            android:id="@+id/sample_process_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Process name"/>
        <TextView
            android:id="@+id/sample_machine_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Machine name"/>
        <TextView
            android:id="@+id/sample_part_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Part name"/>
        <TextView
            android:id="@+id/sample_quantity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Quantity"/>
        <TextView
            android:id="@+id/sample_duration"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Duration"/>
        <TextView
            android:id="@+id/sample_start_date_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Quantity"/>
    </LinearLayout>

</FrameLayout>